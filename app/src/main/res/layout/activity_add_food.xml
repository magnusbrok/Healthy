<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Nutrition.AddFood">

    <ImageButton
        android:id="@+id/fruitsAndVeggies"
        app:srcCompat="@drawable/nutrition_fruitsandveggies"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="128dp"
        android:background="@drawable/homepage_social_button"
        android:backgroundTint="@color/colorTurkis"
        android:text="Frugt/Grønt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/fish"
        app:srcCompat="@drawable/nutritionpage_fish"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="132dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/homepage_social_button"
        android:backgroundTint="@color/colorTurkis"
        android:text="Fisk"
        app:layout_constraintBottom_toTopOf="@+id/water"
        app:layout_constraintStart_toEndOf="@+id/fruitsAndVeggies"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <ImageButton
        android:id="@+id/wholeMeal"
        app:srcCompat="@drawable/nutrition_page_fuldkorn"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="23dp"
        android:layout_marginLeft="23dp"
        android:layout_marginEnd="28dp"
        android:layout_marginRight="28dp"
        android:background="@drawable/rounded_button"
        android:backgroundTint="@color/colorTurkis"
        android:text="Fuldkorn"
        app:layout_constraintBottom_toTopOf="@+id/beverages"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/fish"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.8" />

    <ImageButton
        android:id="@+id/dairy"
        app:srcCompat="@drawable/nutrition_mejeri"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/homepage_social_button"
        android:backgroundTint="@color/colorTurkis"
        android:text="Mejeri"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fruitsAndVeggies" />

    <ImageButton
        android:id="@+id/water"
        app:srcCompat="@drawable/nutrition_page_water"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/homepage_social_button"
        android:backgroundTint="@color/colorTurkis"
        android:text="Vand"
        app:layout_constraintStart_toEndOf="@+id/dairy"
        app:layout_constraintTop_toBottomOf="@+id/fruitsAndVeggies" />

    <ImageButton
        android:id="@+id/beverages"
        app:srcCompat="@drawable/nutrition_page_beverage"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/homepage_social_button"
        android:backgroundTint="@color/colorTurkis"
        android:text="Drikkevarer"
        app:layout_constraintStart_toEndOf="@+id/water"
        app:layout_constraintTop_toBottomOf="@+id/fruitsAndVeggies" />

    <ImageButton
        android:id="@+id/extra"
        app:srcCompat="@drawable/nutrition_page_extra"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="112dp"
        android:background="@drawable/homepage_social_button"
        android:backgroundTint="@color/colorTurkis"
        android:text="+"
        app:layout_constraintStart_toEndOf="@+id/beverages"
        app:layout_constraintTop_toTopOf="@+id/fruitsAndVeggies" />

    <ImageButton
        android:id="@+id/meat"
        app:srcCompat="@drawable/nutrition_page_magertkoed"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="128dp"
        android:background="@drawable/homepage_social_button"
        android:backgroundTint="@color/colorTurkis"
        android:text="Magertkød"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.208"
        app:layout_constraintStart_toEndOf="@+id/wholeMeal"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/done"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="108dp"
        android:text="Færdig!"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>