<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".StartPage">

    <FrameLayout
        android:id="@+id/topMenuFragment"
        android:layout_width="0dp"
        android:layout_height="70dp"
        android:layout_marginBottom="78dp"
        app:layout_constraintBottom_toTopOf="@+id/rewardPoints"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </FrameLayout>

    <ImageButton
        android:id="@+id/button20"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="35dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="35dp"
        app:srcCompat="@drawable/social_page"
        android:background="@drawable/custom_buttons"
        android:backgroundTint="@color/colorGrey"
        android:text="Social"
        app:layout_constraintStart_toEndOf="@+id/button15"
        app:layout_constraintTop_toBottomOf="@+id/button18" />

    <ImageButton
        android:id="@+id/button18"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="35dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="119dp"
        android:layout_marginEnd="35dp"
        android:layout_marginRight="35dp"
        android:text="Nutrition"
        app:srcCompat="@drawable/nutrition_page"
        android:background="@drawable/custom_buttons"
        android:backgroundTint="@color/colorGrey"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button17"
        app:layout_constraintTop_toBottomOf="@+id/topMenuFragment" />

    <ImageButton
        android:id="@+id/button17"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="35dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="119dp"
        android:text="Activity"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/activity_guy_final"
        android:background="@drawable/custom_buttons"
        android:backgroundTint="@color/colorGrey"
        app:layout_constraintTop_toBottomOf="@+id/topMenuFragment" />

    <ImageButton
        android:id="@+id/button15"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="35dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="35dp"
        android:text="Reward"
        app:srcCompat="@drawable/reward_page"
        android:background="@drawable/custom_buttons"
        android:backgroundTint="@color/colorGrey"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button17" />

    <FrameLayout
        android:id="@+id/menuFragment"
        android:layout_width="413dp"
        android:layout_height="78dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>