<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/listViewYourPrizes"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Reward.YourPrizePageFragment">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="69dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="30dp"
        android:text="Dine priser "
        android:textColor="#000000"
        android:textSize="30sp"
        android:textStyle="bold" />

    <ListView
        android:id="@+id/listviewYourPrize"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/border_reward_yourprizes"
        android:divider="@color/colorMustard"
        android:dividerHeight="5dp" />

</LinearLayout>